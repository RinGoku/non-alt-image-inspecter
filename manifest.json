{
    "manifest_version": 2,
    "name": "Non-alt Image Inspecter",
    "version": "1.0.0",
    "description": "alt属性が付与されていない画像をぼやけさせます",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_title": "Sample"
    },
    "content_scripts": [
        {
            "matches": [ "http://*/*", "https://*/*" ],
            "js": [ "script.js" ]
        }
    ]
}